@page
@model PrivacyModel
@{
    ViewData["Title"] = "Ludo Items";
    <h1>@ViewData["Title"]</h1>
}

<style>

    body {
        font-family: Arial, sans-serif;
        background-color: skyblue;
        margin: 0;
        padding: 0;
    }

    header {
        text-align: center;
        background-color: #333;
        color: white;
        padding: 20px 0;
    }

    h1 {
        margin: 0;
    }

    p {
        margin: 5px 0 20px 0;
        font-size: 18px;
    }

    .products {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        padding: 20px;
    }

    .product {
        background-color: white;
        border-radius: 10px;
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        margin: 15px;
        padding: 20px;
        text-align: center;
        width: 200px;
        transition: transform 0.3s ease;
    }

        .product:hover {
            transform: translateY(-5px);
        }

        .product img {
            max-width: 100%;
            border-radius: 10px;
        }

        .product h2 {
            font-size: 20px;
            margin: 15px 0;
        }

        .product p {
            color: #555;
        }

    button {
        background-color: #333;
        color: white;
        border: none;
        padding: 10px 20px;
        border-radius: 5px;
        cursor: pointer;
        transition: background-color 0.3s ease;
    }

        button:hover {
            background-color: #555;
        }
</style>



<section class="products">
    <div class="product">
        <img src="https://c8.alamy.com/comp/2BE1KPA/pieces-and-a-die-on-a-ludo-board-game-2BE1KPA.jpg" alt="Ludo Board">
        <h2>Ludo Board</h2>
        <p>A durable and colorful Ludo board.</p>
        <button>Add to Cart</button>
    </div>
    <div class="product">
        <img src="https://th.bing.com/th/id/OPAC.DWx5q9ESjj7Upg474C474?w=160&h=150&rs=1&o=5&dpr=2&pid=21.1" alt="Ludo Pieces">
        <h2>Ludo Pieces</h2>
        <p>Set of vibrant Ludo pieces.</p>
        <button>Add to Cart</button>
    </div>
    <div class="product">
        <img src="https://th.bing.com/th?id=OIP.CG2FUoe3QgEVzvHsmCKUswHaHa&w=250&h=250&c=8&rs=1&qlt=90&o=6&dpr=2&pid=3.1&rm=2g" alt="Ludo Dice">
        <h2>Ludo Dice</h2>
        <p>Perfectly weighted dice for Ludo.</p>
        <button>Add to Cart</button>
    </div>
    <div class="product">
        <img src="https://th.bing.com/th/id/OIP.bY1e2Rugj-LRDpk-NBEeawHaHa?w=214&h=214&c=7&r=0&o=5&dpr=2&pid=1.7" alt="Ludo Travel Kit">
        <h2>Ludo Travel Kit</h2>
        <p>Portable Ludo set for on-the-go fun.</p>
        <button>Add to Cart</button>
    </div>
    <div class="product">
        <img src="data:image/jpeg;base64," alt="Ludo Strategy Book">
        <h2>Ludo Playing Table</h2>
        <p>4 Leg Table for playing it at ease.</p>
        <button>Add to Cart</button>
    </div>
</section>